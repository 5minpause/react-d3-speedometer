{
  "presets":  [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["IE 11",
          "> 1%",
          "last 2 versions"
          ],
          "uglify": true
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "env": {
    "production": {
        "plugins": [
            "transform-remove-console",
            "transform-runtime"
        ],
        "presets": ["babili"]
    }
  }
}
